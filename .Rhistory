# generate a dataset
generateData <- function(type = 0, n = 30, lambda = 0, sd = 1) {
set.seed(123)
if(type == 0) { # normal
data.set <-rnorm(n*100,lambda,sd)
} else { # poisson
data.set <- rpois(n*100,lambda)
}
newdata <- sample(data.set, n)
return(newdata)
}
