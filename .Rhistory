generateData <- function(type = 0, n = 30, lambda = 0, sd = 1, seed = 222) {
# set.seed(seed)
if(type == 0) { # normal
data.set <-rnorm(n*10,lambda,sd)
} else { # poisson
data.set <- rpois(n*10,lambda)
}
newdata <- sample(data.set, n)
return(newdata)
}
